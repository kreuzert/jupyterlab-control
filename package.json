  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/kreuzert/jupyterlab-control",
  "bugs": {
    "url": "https://github.com/kreuzert/jupyterlab-control/issues"
